{
  "name": "eflow-ts-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch & nodemon build",
    "start": "nodemon --require ts-node/register ./src/index.ts",
    "serve": "node ./build/index.js",
    "migrate:up": "npm run build && sequelize db:migrate",
    "migrate:down": "npm run build && sequelize db:migrate:undo:all",
    "seed:up": "npm run build && sequelize db:seed:all",
    "seed:down": "npm run build && sequelize db:seed:undo:all",
    "test": "npm run test:prepare && NODE_ENV=test mocha -r ts-node/register **/*.spec.ts --exit",
    "dev:prepare": "rimraf ./build && npm run build && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:undo:all && sequelize db:seed:all",
    "test:prepare": "rimraf ./build && npm run build && NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "stage:prepare": "rimraf ./build && npm run build && NODE_ENV=stage sequelize db:migrate:undo:all && NODE_ENV=stage sequelize db:migrate && NODE_ENV=stage sequelize db:seed:undo:all && NODE_ENV=stage sequelize db:seed:all"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^3.0.5",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/pg": "^7.4.10",
    "@types/sequelize": "^4.27.25",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.0",
    "codelyzer": "^4.4.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/csvtojson": "^1.1.5",
    "@types/request": "^2.47.1",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai-things": "^0.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "csvtojson": "^2.0.8",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "rand-token": "^0.4.0",
    "request": "^2.88.0",
    "rxjs": "^6.3.3",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "uuid": "^3.3.2"
  }
}
